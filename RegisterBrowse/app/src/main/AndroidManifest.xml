<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thomas.registerbrowse">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="先注册还是先浏览？"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectImage">
            <intent-filter>
                <action android:name="liu" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:scheme="liu"
                    android:host="com.android.thomas"
                    android:port="686"
                    />
            </intent-filter>
        </activity>
        <activity android:name=".OtherActivity"></activity>
        <activity android:name=".MixView">
            <intent-filter>
                <action android:name="liu" />
                <category android:name="android.intent.category.DEFAULT" />
             <data android:scheme="liu"
                 android:host="com.android.thomas"
                 android:port="868"
                 />
            </intent-filter>
        </activity>
    </application>
</manifest>